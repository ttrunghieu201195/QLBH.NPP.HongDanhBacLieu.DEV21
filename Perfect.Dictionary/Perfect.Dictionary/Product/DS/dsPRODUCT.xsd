                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT p.Product_ID,p.Product_Name, p.Quantity, p.Org_Price, p.Product_Type_ID,u.Unit_Name,p.Sale_Price,
       p.Retail_Price, p.MinStock, p.MaxStock, p.LimitOrders,s.Stock_Name,pg.ProductGroup_Name,p.[Description],p.[Active],p.Photo,u.Unit_ID
FROM    PRODUCT p INNER JOIN UNIT u ON u.Unit_ID = p.Unit INNER JOIN STOCK s ON p.Provider_ID = s.Stock_ID
