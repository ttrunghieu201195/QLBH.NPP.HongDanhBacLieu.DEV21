            <MainSource>
              <DbSource ConnectionRef="hongdanhblConnectionString (Settings)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        A.Product_ID, A.Product_Name, A.Quantity, CASE WHEN A.Org_Price IS NULL THEN 0 ELSE A.Org_Price END AS Org_Price, A.Amount, PRODUCT.Product_Type_ID, 
                         UNIT.Unit_Name, PRODUCT.Sale_Price, PRODUCT.Retail_Price, PRODUCT.MinStock, PRODUCT.MaxStock, PRODUCT.LimitOrders, STOCK.Stock_Name, 
                         PRODUCT_GROUP.ProductGroup_Name, PRODUCT.Description, PRODUCT.Active, PRODUCT.Product_Group_ID, PRODUCT.Photo
FROM            (SELECT        b.Product_ID, b.Product_Name, CASE WHEN c.[Quantity] IS NULL THEN 0 ELSE c.[Quantity] END AS Quantity, 
                                                    CASE WHEN c.[Quantity] = 0 THEN 0 ELSE c.[Amount] / c.[Quantity] END AS Org_Price, CASE WHEN c.[Amount] IS NULL 
                                                    THEN 0 ELSE c.[Amount] END AS Amount
                          FROM            (SELECT        Product_ID, SUM(Quantity) AS Quantity, SUM(Amount) AS Amount
                                                    FROM            INVENTORY
                                                    WHERE        (Quantity &lt;&gt; 0)
                                                    GROUP BY Product_ID) AS c FULL OUTER JOIN
                                                    PRODUCT AS b ON c.Product_ID = b.Product_ID
                          WHERE        (b.Active = 1)) AS A INNER JOIN
                         PRODUCT ON A.Product_ID = PRODUCT.Product_ID INNER JOIN
                         STOCK ON PRODUCT.Provider_ID = STOCK.Stock_ID INNER JOIN
                         PRODUCT_GROUP ON PRODUCT.Product_Group_ID = PRODUCT_GROUP.ProductGroup_ID INNER JOIN
                         UNIT ON PRODUCT.Unit = UNIT.Unit_ID
WHERE        (PRODUCT.Product_Group_ID = @Product_Group_ID)
ORDER BY A.Product_Name</CommandText>
                    <Parameters>
                      <Parameter AllowDbNull="true" AutogeneratedName="Product_Group_ID" ColumnName="Product_Group_ID" DataSourceName="ERP31.dbo.PRODUCT" DataTypeServer="varchar(20)" DbType="AnsiString" Direction="Input" ParameterName="@Product_Group_ID" Precision="0" ProviderType="VarChar" Scale="0" Size="20" SourceColumn="Product_Group_ID" SourceColumnNullMapping="false" SourceVersion="Current" />
                    </Parameters>
                  </DbCommand>
                </SelectCommand>
