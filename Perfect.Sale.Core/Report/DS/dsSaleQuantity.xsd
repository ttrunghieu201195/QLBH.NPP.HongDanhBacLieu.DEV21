                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT sod.Product_ID,
       p.Product_Name,
       u.Unit_Name,
       sod.Quantity,
       sod.UnitPrice,
       sod.Amount,
       so.Vat,
       so.DiscountRate,
       (sod.Amount * so.DiscountRate / 100) AS DiscountAmount,
       ((sod.Amount -(sod.Amount * so.DiscountRate / 100) ) * so.Vat / 100) AS VatAmount,
       ( sod.Amount + ((sod.Amount -(sod.Amount * so.DiscountRate / 100) ) * so.Vat / 100) - (sod.Amount * so.DiscountRate / 100) ) AS TotalAmount,
       so.Employee_ID,
       so.ID, 
       so.RefDate
FROM   STOCK_OUTWARD_DETAIL sod
       LEFT JOIN STOCK_OUTWARD so
            ON  so.ID = sod.Outward_ID
       LEFT JOIN PRODUCT p
            ON  p.Product_ID = sod.Product_ID
       LEFT JOIN UNIT u
            ON  u.Unit_ID = sod.Unit
WHERE  DATEDIFF(DAY, @FromDate, so.RefDate) &gt;= 0
       AND DATEDIFF(DAY, @ToDate, so.RefDate) &lt;= 0</CommandText>
