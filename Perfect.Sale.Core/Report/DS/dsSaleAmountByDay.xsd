          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="Buy_Revenue_By_DateTableAdapter" GeneratorDataComponentClassName="Buy_Revenue_By_DateTableAdapter" Name="Buy_Revenue_By_Date" UserDataComponentName="Buy_Revenue_By_DateTableAdapter">
            <MainSource>
              <DbSource ConnectionRef="ERP_THANHTAMConnectionString (Settings)" DbObjectName="SELECT CASE WHEN SALE.RefDate IS NULL THEN BUY.RefDate ELSE SALE.RefDate END AS RefDate, CASE WHEN SALE.TotalSaleAmount IS NULL THEN 0 ELSE SALE.TotalSaleAmount END AS TotalSaleAmount, CASE WHEN BUY.TotalBuyAmount IS NULL THEN 0 ELSE BUY.TotalBuyAmount END AS TotalBuyAmount FROM ( SELECT DATEADD(dd, DATEDIFF(d, 0, so.RefDate), 0) AS RefDate, SUM(so.Amount) AS TotalSaleAmount FROM STOCK_OUTWARD so WHERE DATEDIFF(DAY, @FromDate, so.RefDate) &gt;= 0 AND DATEDIFF(DAY, @ToDate, so.RefDate) &lt;= 0 GROUP BY DATEADD(dd, DATEDIFF(d, 0, so.RefDate), 0) )SALE FULL OUTER JOIN ( SELECT DATEADD(dd, DATEDIFF(d, 0, si.RefDate), 0) AS RefDate, SUM(si.Amount) AS TotalBuyAmount FROM STOCK_INWARD si WHERE si.RefType != 0 AND DATEDIFF(DAY, @FromDate, si.RefDate) &gt;= 0 AND DATEDIFF(DAY, @ToDate, si.RefDate) &lt;= 0 GROUP BY DATEADD(dd, DATEDIFF(d, 0, si.RefDate), 0) )BUY ON SALE.RefDate = BUY.RefDate" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="true" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT REVENUE.RefDate,
       REVENUE.TotalBuyAmount,
       REVENUE.TotalSaleAmount,
       (REVENUE.TotalBuyAmount - REVENUE.TotalSaleAmount) AS Gap
FROM   (
           SELECT CASE 
                       WHEN SALE.RefDate IS NULL THEN BUY.RefDate
                       ELSE SALE.RefDate
                  END AS RefDate,
                  CASE 
                       WHEN SALE.TotalSaleAmount IS NULL THEN 0
                       ELSE SALE.TotalSaleAmount
                  END AS TotalSaleAmount,
                  CASE 
                       WHEN BUY.TotalBuyAmount IS NULL THEN 0
                       ELSE BUY.TotalBuyAmount
                  END AS TotalBuyAmount
           FROM   (
                      SELECT DATEADD(dd, DATEDIFF(d, 0, so.RefDate), 0) AS 
                             RefDate,
                             SUM(so.Amount) AS TotalSaleAmount
                      FROM   STOCK_OUTWARD so
                      WHERE  DATEDIFF(DAY, @FromDate, so.RefDate) &gt;= 0
                             AND DATEDIFF(DAY, @ToDate, so.RefDate) &lt;= 0
                      GROUP BY
                             DATEADD(dd, DATEDIFF(d, 0, so.RefDate), 0)
                  )SALE
                  FULL OUTER JOIN (
                           SELECT DATEADD(dd, DATEDIFF(d, 0, si.RefDate), 0) AS 
                                  RefDate,
                                  SUM(si.Amount) AS TotalBuyAmount
                           FROM   STOCK_INWARD si
                           WHERE  si.RefType != 0
                                  AND DATEDIFF(DAY, @FromDate, si.RefDate) &gt;= 0
                                  AND DATEDIFF(DAY, @ToDate, si.RefDate) &lt;= 0
                           GROUP BY
                                  DATEADD(dd, DATEDIFF(d, 0, si.RefDate), 0)
                       )BUY
                       ON  SALE.RefDate = BUY.RefDate
       )REVENUE
ORDER BY
       REVENUE.RefDate</CommandText>
