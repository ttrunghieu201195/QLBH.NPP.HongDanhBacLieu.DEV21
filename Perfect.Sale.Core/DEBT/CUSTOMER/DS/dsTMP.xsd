                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT d.ID,
       d.RefID,
       d.RefDate,
       d.RefType,
       d.RefStatus,
       d.TermID,
       (SELECT ct.Name FROM CASH_TERM ct WHERE ct.ID = d.TermID) AS [TermName],
       d.CustomerID,
       c.CustomerName,
       c.CustomerAddress,
       d.Amount,
       d.Payment,
       d.Balance
FROM   DEBT d
       LEFT OUTER JOIN CUSTOMER c
            ON  d.CustomerID = c.Customer_ID
WHERE  d.RefType = @RefType
       AND d.TermID = @CashMethod
