            <Sources>
              <DbSource ConnectionRef="hongdanhblConnectionString1 (Settings)" DbObjectName="" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill_InitInward" GenerateMethods="Both" GenerateShortCommands="true" GeneratorGetMethodName="GetData_InitInward" GeneratorSourceName="Fill_InitInward" GetMethodModifier="Public" GetMethodName="GetData_InitInward" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="true" UserGetMethodName="GetData_InitInward" UserSourceName="Fill_InitInward">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>DECLARE @count BIGINT
	
SELECT @count = COUNT(*)
FROM   INVENTORY i
WHERE  i.Quantity &lt;&gt; 0
	
IF (@count &gt; 0)
BEGIN
    SELECT B.Product_ID,
           B.Product_Name,
           s.Stock_ID,
           s.Stock_Name,
           B.Quantity,
           CASE 
                WHEN B.Org_Price IS NULL THEN 0
                ELSE B.Org_Price
           END Org_Price,
           CASE 
                WHEN B.Sale_Price IS NULL THEN 0
                ELSE B.Sale_Price
           END Sale_Price,
           CASE 
                WHEN B.Retail_Price IS NULL THEN 0
                ELSE B.Retail_Price
           END Retail_Price,
           Amount,
           CAST(B.[Active] AS INT) AS ActiveInt
    FROM   (
               SELECT b.Product_ID,
                      CASE 
                           WHEN A.Stock_ID IS NULL THEN b.Provider_ID
                           ELSE A.Stock_ID
                      END AS Stock_ID,
                      --b.Provider_ID AS Stock_ID,
                      
                      b.Product_Name,
                      CASE 
                           WHEN [a].[Quantity] IS NULL THEN 0
                           ELSE [a].[Quantity]
                      END AS Quantity,
                      b.Org_Price AS Org_Price,
                      b.Sale_Price AS Sale_Price,
                      b.Retail_Price AS Retail_Price,
                      CAST(0 AS MONEY) AS Amount,
                      b.[Active]
               FROM   (
                          SELECT Product_ID,
                                 Stock_ID,
                                 SUM(Quantity) AS Quantity,
                                 SUM(Amount) AS Amount
                          FROM   INVENTORY
                          WHERE  (Quantity &lt;&gt; 0)
                          GROUP BY
                                 Product_ID,
                                 Stock_ID
                      ) A
                      FULL OUTER JOIN PRODUCT b
                           ON  a.Product_ID = b.Product_ID
           ) B
           INNER JOIN STOCK s
                ON  B.Stock_ID = s.Stock_ID
    ORDER BY
           B.Product_Name
END
ELSE
BEGIN
    SELECT p.Product_ID,
           p.Product_Name,
           p.Provider_ID AS Stock_ID,
           s.Stock_Name,
           p.Quantity,
           CAST(0 AS MONEY) Org_Price,
           CAST(0 AS MONEY) Sale_Price,
           CAST(0 AS MONEY) Retail_Price,
           CAST(0 AS MONEY) Amount,
           CAST(p.[Active] AS INT) AS ActiveInt
    FROM   PRODUCT p
           INNER JOIN STOCK s
                ON  p.Provider_ID = s.Stock_ID
    ORDER BY
           p.Product_Name
END</CommandText>
