            <MainSource>
              <DbSource ConnectionRef="ERP_THANHTAMConnectionString (Settings)" DbObjectName="SELECT b.Product_ID, b.Product_Name, CASE WHEN [a].[Quantity] IS NULL THEN 0 ELSE [a].[Quantity] END AS Quantity, CASE WHEN [a].[Quantity] =0 THEN 0 ELSE [a].[Amount]/[a].[Quantity] END AS Org_Price, CASE WHEN [a].[Amount] IS NULL THEN 0 ELSE [a].[Amount] END AS Amount FROM ( ( SELECT Product_ID, SUM(Quantity) AS Quantity, SUM(Amount) AS Amount FROM INVENTORY WHERE (Quantity &lt;&gt; 0) GROUP BY Product_ID ) A FULL OUTER JOIN PRODUCT AS b ON a.Product_ID = b.Product_ID ) WHERE (b.Active = 1) AND (b.Product_Type_ID = @Product_Type_ID)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="true" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        Product_ID,
			  Product_Name,
			  Quantity,
			  CASE WHEN Org_Price IS NULL THEN 0 ELSE Org_Price END Org_Price,
			  Amount
			  
FROM        
		(
			
			SELECT
						b.Product_ID,
						b.Product_Name,
						CASE WHEN [a].[Quantity] IS NULL THEN 0 ELSE [a].[Quantity] END AS Quantity,
						CASE WHEN [a].[Quantity] =0 THEN 0 ELSE [a].[Amount]/[a].[Quantity] END AS Org_Price,
						CASE WHEN [a].[Amount] IS NULL THEN 0 ELSE [a].[Amount] END AS Amount
				FROM
					(	
					
						( SELECT   Product_ID, SUM(Quantity) AS Quantity, SUM(Amount) AS Amount
						 FROM    INVENTORY
						 WHERE   (Quantity &lt;&gt; 0)
						 GROUP BY Product_ID 
						 ) A FULL OUTER JOIN PRODUCT AS b ON a.Product_ID = b.Product_ID						   
					) 
					
				WHERE        (b.Active = 1)  AND (b.Product_Type_ID = @Product_Type_ID)
				
				) A
					
	
							
</CommandText>
