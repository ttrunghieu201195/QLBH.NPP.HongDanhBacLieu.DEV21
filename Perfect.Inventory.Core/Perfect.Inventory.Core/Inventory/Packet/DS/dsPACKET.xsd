                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        CAST(0 AS bit) AS IsPrint, CAST(0 AS bit) AS IsBarcode, STOCK_TRANSFER.ID, STOCK_TRANSFER.RefDate, STOCK_TRANSFER.RefType, 
