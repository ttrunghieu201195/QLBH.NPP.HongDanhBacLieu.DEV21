                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT sid1.Product_ID,
       p.Product_Name,
       u.Unit_Name,
       sid1.Quantity,
       sid1.Amount * si.ExchangeRate AS Amount,
       CASE 
            WHEN sid1.Quantity = 0 THEN 0
            ELSE sid1.Amount * si.ExchangeRate /
                 sid1.Quantity
       END AS UnitPrice,
       pg.ProductGroup_ID,
       pg.ProductGroup_Name,
       s.Stock_Name,
       s.Stock_ID,
       si.RefDate,
       si.ID,
       p.Unit,
       si.RefType
FROM   STOCK_INWARD_DETAIL sid1
       LEFT OUTER JOIN STOCK s
            ON  sid1.Stock_ID = s.Stock_ID
       LEFT OUTER JOIN PRODUCT p
            ON  sid1.Product_ID = p.Product_ID
       LEFT OUTER JOIN PRODUCT_GROUP pg
            ON  p.Product_Group_ID = pg.ProductGroup_ID
       LEFT OUTER JOIN STOCK_INWARD si
            ON  sid1.Inward_ID = si.ID
       LEFT OUTER JOIN UNIT u
            ON  sid1.Unit = u.Unit_ID
WHERE  si.RefType = 0
       AND (DATEDIFF(DAY, @FromDate, si.RefDate) &gt;= 0)
       AND (DATEDIFF(DAY, @ToDate, si.RefDate) &lt;= 0)
ORDER BY
       si.RefDate DESC</CommandText>
