                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText> 
SELECT so.ID,
       so.RefDate,
       so.Charge                     AS Totals,
       so.Notes, ca.CarNumber AS CarNumber,
       so.Customer_ID,
       so.Ref_OrgNo,TermID,
       so.Reason,
       CAST(0 AS BIT)                AS IsPrint,
       CAST(0 AS BIT)                AS IsBarcode,
       so.Stock_ID,
       so.CustomerName,
       so.CustomerAddress,
       (so.Amount - so.VatAmount + so.Discount) AS [Charge],
       so.Vat,
       so.VatAmount,
       so.DiscountRate,
       so.Discount,
       so.Amount,
       so.OtherDiscount,
       so.OtherDiscount + so.Amount  AS Total,
       CASE 
            WHEN n.Name IS NULL THEN so.[Description]
            ELSE n.Name
       END                           AS Description,
       so.RefType,
       CASE so.RefType
            WHEN 2 THEN N'Xuất Sản Xuất'
            WHEN 46 THEN N'Xuất Thay Bao Bì'
            WHEN 47 THEN N'Xuất Tái Chế'
       END                           AS Descr,
       so.RefStatus,
       r.Name                        AS RouteName,
       CASE 
            WHEN n.[Description] IS NULL THEN 'White'
            ELSE n.[Description]
       END                           AS ColorName,
       so.Profit,
       cg.Customer_Group_Name
FROM   STOCK_OUTWARD so
       LEFT OUTER JOIN [ROUTE] r
            ON  r.ID = so.Department_ID
      LEFT OUTER JOIN CARS ca ON ca.ID = so.Branch_ID
       LEFT OUTER JOIN NOTES n
            ON  n.ID = so.Description
      LEFT OUTER JOIN CUSTOMER c ON c.Customer_ID = so.Customer_ID
      LEFT OUTER  JOIN CUSTOMER_GROUP  AS cg ON  cg.Customer_Group_ID = c.Customer_Group_ID
WHERE  (DATEDIFF(DAY, @FromDate, so.RefDate) &gt;= 0)
       AND (DATEDIFF(DAY, @ToDate, so.RefDate) &lt;= 0)
ORDER BY
       so.Sorted                        DESC</CommandText>
