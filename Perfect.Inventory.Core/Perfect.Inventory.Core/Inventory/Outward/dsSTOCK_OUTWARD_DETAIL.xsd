          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="SanLuongTuyenTableAdapter" GeneratorDataComponentClassName="SanLuongTuyenTableAdapter" Name="SanLuongTuyen" UserDataComponentName="SanLuongTuyenTableAdapter">
            <MainSource>
              <DbSource ConnectionRef="hongdanhblConnectionString1 (Settings)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT A.Product_ID,
       p.Product_Name,  
       A.Quantity AS TotalQuantity,
       --uuu.Unit_Name AS Unit0,
       ----
       CASE 
            WHEN pu.UnitConvert IS NULL THEN NULL
            ELSE pu.UnitConvert
       END                            AS ConvertValues,
       ----
       CASE 
            WHEN pu.UnitConvert IS NULL THEN A.Quantity
            ELSE A.Quantity % pu.UnitConvert
       END                            AS Quantity,
       u.Unit_Name                    AS Unit,
       ------
       CASE 
            WHEN pu.UnitConvert IS NULL THEN NULL
            ELSE (A.Quantity -(A.Quantity % pu.UnitConvert)) / pu.UnitConvert
       END                            AS QuantityConvert,
       CASE 
            WHEN pu.UnitConvert IS NULL THEN NULL
            ELSE uu.Unit_Name
       END                            AS UnitConvert,
       s.Stock_Name,
       r.Name AS RouteName,
       pg.ProductGroup_Name AS GroupName
FROM   (
           SELECT sod.Product_ID,
                  sod.Unit,
                  SUM(sod.Quantity)  AS Quantity,
                  sod.Stock_ID,
                  so.Department_ID   AS RouteID
           FROM   STOCK_OUTWARD_DETAIL sod
                  LEFT OUTER JOIN STOCK_OUTWARD so
                       ON  sod.Outward_ID = so.ID
           WHERE  so.RefType = 2 
				  AND (DATEDIFF(DAY, @FromDate, so.RefDate) &gt;= 0)
                  AND (DATEDIFF(DAY, @ToDate, so.RefDate) &lt;= 0)
                   
           GROUP BY
                  sod.Product_ID,
                  sod.Unit,
                  sod.Stock_ID,
                  so.Department_ID
       )A
       
       LEFT OUTER JOIN PRODUCT p
            ON  p.Product_ID = A.Product_ID
            
       LEFT JOIN PRODUCT_UNIT  AS pu
            ON  pu.Product_ID = A.Product_ID
            AND pu.Unit_ID = A.Unit
       LEFT OUTER JOIN UNIT    AS u
            ON  u.Unit_ID = A.Unit
       LEFT OUTER JOIN UNIT    AS uu
            ON  uu.Unit_ID = pu.UnitConvert_ID
       --LEFT OUTER JOIN UNIT    AS uuu
            --ON  uuu.Unit_ID = p.Unit  
            
       LEFT OUTER JOIN [ROUTE] r
            ON  r.ID = A.RouteID
       LEFT OUTER JOIN STOCK s
            ON  s.Stock_ID = A.Stock_ID
       LEFT OUTER JOIN PRODUCT_GROUP pg
            ON  pg.ProductGroup_ID = p.Product_Group_ID
            
 ORDER BY
       A.RouteID,
       A.Product_ID</CommandText>
