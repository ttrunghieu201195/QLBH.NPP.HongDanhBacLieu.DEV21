          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="STOCK_OUTWARD_DETAIL_ByDateTableAdapter" GeneratorDataComponentClassName="STOCK_OUTWARD_DETAIL_ByDateTableAdapter" Name="STOCK_OUTWARD_DETAIL_ByDate" UserDataComponentName="STOCK_OUTWARD_DETAIL_ByDateTableAdapter">
            <MainSource>
              <DbSource ConnectionRef="hongdanhblConnectionString1 (Settings)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>
SELECT sod.Product_ID,
       p.Product_Name,
       pb.Name AS ProductBrand,
       (
           SELECT u.Unit_Name
           FROM   UNIT u
           WHERE  u.Unit_ID = sod.Unit
       )                             AS Unit_Name,
       sod.Quantity,
       sod.Cost,
       sod.Profit,
       sod.Amount * so.ExchangeRate  AS Amount,
       sod.UnitPrice,
       sod.UnitPrice                 AS Price,
       sod.DiscountRate,
       sod.Discount,
       sod.Vat,
       sod.VatAmount,
       sod.Charge,
       p.Product_Group_ID,
       (
           SELECT pg.ProductGroup_Name
           FROM   PRODUCT_GROUP pg
           WHERE  pg.ProductGroup_ID = p.Product_Group_ID
       )                             AS ProductGroup_Name,
       (
           SELECT s.Stock_Name
           FROM   STOCK s
           WHERE  s.Stock_ID = sod.Stock_ID
       )                             AS Stock_Name,
       sod.Stock_ID,
       so.RefDate,
       so.ID,
       sod.Unit,
       so.RefStatus,
       so.RefType,
       so.Customer_ID,
       so.CustomerName,
       sod.Height,sod.IME,
       sod.Width,
       sod.Orgin,
       r.Name                        AS RouteName,
       CASE 
            WHEN n.Name IS NULL THEN so.[Description]
            ELSE n.Name
       END                           AS SalesStatus,
       sod.Serial,
       CASE 
            WHEN p.Quantity IS NULL OR p.Quantity = 0 THEN 0
            WHEN sod.Unit = p.Unit THEN p.Quantity * sod.Quantity
            ELSE p.Quantity * sod.Quantity * pu.UnitConvert
       END                           AS TinhSo,
       CASE 
            WHEN p.Quantity IS NULL OR p.Quantity = 0 THEN 0
            WHEN sod.Unit = p.Unit THEN (sod.UnitPrice * sod.Quantity) -(p.Quantity * sod.Quantity)
            ELSE (sod.UnitPrice * sod.Quantity) -(p.Quantity * sod.Quantity * pu.UnitConvert)
       END                           AS Lech,
       CASE 
            WHEN p.VAT_ID IS NULL OR p.VAT_ID = 0 THEN 0
            ELSE sod.Amount * p.VAT_ID / 100
       END                           AS HoaHong,
       CASE 
            WHEN n.[Description] IS NULL THEN 'White'
            ELSE n.[Description]
       END                           AS ColorName
FROM   STOCK_OUTWARD_DETAIL sod
       LEFT OUTER JOIN PRODUCT p
            ON  sod.Product_ID = p.Product_ID
            LEFT OUTER JOIN PRODUCT_BRAND AS pb
            ON p.Color = pb.ID
       LEFT OUTER JOIN STOCK_OUTWARD so
            ON  sod.Outward_ID = so.ID
       LEFT OUTER JOIN [ROUTE] r
            ON  r.ID = so.Department_ID
       LEFT OUTER JOIN NOTES n
            ON  n.ID = so.Description
            
       LEFT OUTER JOIN PRODUCT_UNIT pu
            ON  p.Product_ID = pu.Product_ID
            AND p.Unit = pu.Unit_ID
WHERE  (DATEDIFF(DAY, @FromDate, so.RefDate) &gt;= 0)
       AND (DATEDIFF(DAY, @ToDate, so.RefDate) &lt;= 0)
ORDER BY
       so.RefDate                       DESC</CommandText>
