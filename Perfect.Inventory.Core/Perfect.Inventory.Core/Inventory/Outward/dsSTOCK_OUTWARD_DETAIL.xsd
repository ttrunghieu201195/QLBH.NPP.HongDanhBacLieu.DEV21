            <MainSource>
              <DbSource ConnectionRef="hongdanhblConnectionString1 (Settings)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="true" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT sod.ID,
       sod.Outward_ID,
       sod.Stock_ID,
       sod.RefType,
       sod.Product_ID,
       sod.Product_ID AS ItemID,
       p.Product_Name,
       p.Product_Type_ID,
       sod.Vat,
       sod.Unit,
       u.Unit_Name,
       sod.UnitConvert,
       sod.CurrentQty,
       sod.Quantity,
       sod.UnitPrice,
       (sod.UnitPrice - sod.UnitPrice * sod.DiscountRate / 100) AS PriceDiscount,
       (sod.UnitPrice - sod.UnitPrice * sod.Vat / 100) AS PriceVat,
       sod.Amount,
       sod.QtyConvert,
       sod.DiscountRate,
       sod.Discount,
       sod.Charge,
       sod.Cost,
       sod.Profit,
       sod.Batch,
       sod.Serial,
       sod.ChassyNo,
       sod.IME,
       sod.Width,
       sod.Height,
       sod.Orgin,
       sod.[Size],
       sod.StoreID,
       sod.Description,
       sod.Sorted,
       sod.Active
FROM   STOCK_OUTWARD_DETAIL sod
       LEFT OUTER JOIN PRODUCT p
            ON  sod.Product_ID = p.Product_ID
       LEFT OUTER JOIN UNIT u
            ON  sod.Unit = u.Unit_ID
WHERE  (sod.Outward_ID = @Outward_ID)
ORDER BY
       sod.Sorted</CommandText>
