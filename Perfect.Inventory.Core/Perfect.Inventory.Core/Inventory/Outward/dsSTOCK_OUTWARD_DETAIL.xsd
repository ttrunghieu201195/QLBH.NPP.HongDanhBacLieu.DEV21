          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="INVOICE_STATUSTableAdapter" GeneratorDataComponentClassName="INVOICE_STATUSTableAdapter" Name="INVOICE_STATUS" UserDataComponentName="INVOICE_STATUSTableAdapter">
            <MainSource>
              <DbSource ConnectionRef="hongdanhblConnectionString1 (Settings)" DbObjectName="SELECT COUNT(so.ID) AS Quantity, CASE WHEN so.[Description] IS NULL THEN 'NEW' WHEN so.[Description] = '' THEN 'NEW' WHEN so.[Description] = N'Ứng Hàng' THEN 'UH' WHEN so.[Description] = N'Trả Tiền Mặt' THEN 'TTM' WHEN so.[Description] = N'Đã Giao' THEN 'DG' ELSE so.[Description] END AS StatusID FROM STOCK_OUTWARD AS so WHERE DATEDIFF(DAY, @FromDate, so.RefDate) &gt;= 0 AND DATEDIFF(DAY, @ToDate, so.RefDate) &lt;= 0 GROUP BY CASE WHEN so.[Description] IS NULL THEN 'NEW' WHEN so.[Description] = '' THEN 'NEW' WHEN so.[Description] = N'Ứng Hàng' THEN 'UH' WHEN so.[Description] = N'Trả Tiền Mặt' THEN 'TTM' WHEN so.[Description] = N'Đã Giao' THEN 'DG' ELSE so.[Description] END" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>--DECLARE @FromDate DATETIME
--DECLARE @ToDate DATETIME
--SET @FromDate ='07/30/2021'
--SET @ToDate = '08/10/2021'

SELECT A.StatusID             AS ID,
       CASE 
            WHEN n.Name IS NULL THEN N'Đơn mới'
            ELSE n.Name
       END                    AS Name,
       CASE 
            WHEN n.Name IS NULL THEN N'Transparent'
            ELSE n.[Description]
       END                    AS ColorName,
       A.Quantity
FROM   (
           SELECT COUNT(so.ID)   AS Quantity,
                  CASE 
                       WHEN so.[Description] IS NULL THEN 'NEW'
                       WHEN so.[Description] = '' THEN 'NEW'
                       WHEN so.[Description] = N'Ứng Hàng' THEN 'UH'
                       WHEN so.[Description] = N'Trả Tiền Mặt' THEN 'TTM'
                       WHEN so.[Description] = N'Đã Giao' THEN 'DG'
                       ELSE so.[Description]
                  END            AS StatusID
           FROM   STOCK_OUTWARD  AS so
           WHERE  DATEDIFF(DAY, @FromDate, so.RefDate) &gt;= 0
                  AND DATEDIFF(DAY, @ToDate, so.RefDate) &lt;= 0
           GROUP BY
                  CASE 
                       WHEN so.[Description] IS NULL THEN 'NEW'
                       WHEN so.[Description] = '' THEN 'NEW'
                       WHEN so.[Description] = N'Ứng Hàng' THEN 'UH'
                       WHEN so.[Description] = N'Trả Tiền Mặt' THEN 'TTM'
                       WHEN so.[Description] = N'Đã Giao' THEN 'DG'
                       ELSE so.[Description]
                  END
       ) A
       LEFT OUTER JOIN NOTES  AS n
            ON  A.StatusID = n.ID
ORDER BY
       n.Name</CommandText>
