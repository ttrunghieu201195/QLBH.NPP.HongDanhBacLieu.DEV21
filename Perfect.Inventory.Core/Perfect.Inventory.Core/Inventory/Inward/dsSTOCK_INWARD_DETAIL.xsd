       STOCK_INWARD_DETAIL.Stock_ID,
       STOCK_INWARD_DETAIL.Limit,
       STOCK_INWARD_DETAIL.Vat,
       STOCK_INWARD_DETAIL.VatAmount,
       STOCK_INWARD_DETAIL.Width,
