              <DbSource ConnectionRef="hongdanhblConnectionString1 (Settings)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT     sid1.Product_ID, p.Product_Name,
                          (SELECT     Unit_Name
                            FROM          UNIT AS u
                            WHERE      (Unit_ID = sid1.Unit)) AS Unit_Name, sid1.Quantity, sid1.Amount * si.ExchangeRate AS Amount, 
                      CASE WHEN sid1.Quantity = 0 THEN 0 ELSE sid1.Amount * si.ExchangeRate / sid1.Quantity END AS UnitPrice, p.Product_Group_ID,
                          (SELECT     ProductGroup_Name
                            FROM          PRODUCT_GROUP AS pg
                            WHERE      (ProductGroup_ID = p.Product_Group_ID)) AS ProductGroup_Name,
                          (SELECT     Stock_Name
                            FROM          STOCK AS s
                            WHERE      (Stock_ID = sid1.Stock_ID)) AS Stock_Name, sid1.Stock_ID, si.RefDate, si.ID, sid1.Unit, si.RefStatus, si.RefType, si.Customer_ID, si.CustomerName,sid1.Height,sid1.Width, sid1.DiscountRate, sid1.Discount, sid1.Orgin
FROM         STOCK_INWARD_DETAIL AS sid1 LEFT OUTER JOIN
                      PRODUCT AS p ON sid1.Product_ID = p.Product_ID LEFT OUTER JOIN
                      STOCK_INWARD AS si ON sid1.Inward_ID = si.ID
WHERE     (si.RefType &lt;&gt; 0) AND (DATEDIFF(DAY, @FromDate, si.RefDate) &gt;= 0) AND (DATEDIFF(DAY, @ToDate, si.RefDate) &lt;= 0)
ORDER BY si.RefDate DESC</CommandText>
