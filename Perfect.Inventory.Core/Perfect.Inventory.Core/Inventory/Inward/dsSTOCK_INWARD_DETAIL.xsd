       STOCK_INWARD_DETAIL.UnitConvert,
       STOCK_INWARD_DETAIL.DiscountRate,
       STOCK_INWARD_DETAIL.Discount,
       STOCK_INWARD_DETAIL.Charge,
       STOCK_INWARD_DETAIL.Description,
       STOCK_INWARD_DETAIL.Sorted,
       STOCK_INWARD_DETAIL.Active,
       STOCK_INWARD_DETAIL.StoreID,
       STOCK_INWARD_DETAIL.QtyConvert,
       STOCK_INWARD_DETAIL.RefType,
       STOCK_INWARD_DETAIL.ID,
       PRODUCT.Product_Type_ID,
       PRODUCT.Product_Name,
       UNIT.Unit_Name,
