                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT ID,
           RefDate,
           Customer_ID,
           Ref_OrgNo, 
           CASE 
                WHEN Reason = N'Nhập Kho'THEN ''
                ELSE Reason
           END AS Reason,


           CAST(0 AS BIT) AS IsPrint,
           CAST(0 AS BIT) AS IsBarcode,
           Stock_ID,
           CustomerName,
           CustomerAddress,
           Amount,
           [Description],
           RefType,
           CASE RefType
                WHEN 1 THEN N'Nhập Sản Xuất'
                WHEN 44 THEN N'Nhập Thay Bao Bì'
                WHEN 45 THEN N'Nhập Tái Chế'
           END AS Descr,
           RefStatus,
           Vat, 
           VatAmount,
           Charge
    FROM   STOCK_INWARD
    WHERE  STOCK_INWARD.RefType &lt;&gt; 0
			AND (DATEDIFF(DAY, @FromDate, RefDate) &gt;= 0)
           AND (DATEDIFF(DAY, @ToDate, RefDate) &lt;= 0)
    ORDER BY
