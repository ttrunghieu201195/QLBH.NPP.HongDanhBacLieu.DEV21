                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        CUSTOMER.Customer_ID, CUSTOMER.CustomerName, STOCK_OUTWARD.ID, STOCK_OUTWARD.RefDate, PRODUCT.Product_ID, PRODUCT.Product_Name, 
                         UNIT.Unit_Name, STOCK_OUTWARD_DETAIL.Quantity, STOCK_OUTWARD_DETAIL.UnitPrice, STOCK_OUTWARD_DETAIL.Amount, 
                         STOCK_OUTWARD_DETAIL.Cost
FROM            STOCK_OUTWARD_DETAIL INNER JOIN
                         PRODUCT ON STOCK_OUTWARD_DETAIL.Product_ID = PRODUCT.Product_ID INNER JOIN
                         STOCK_OUTWARD ON STOCK_OUTWARD_DETAIL.Outward_ID = STOCK_OUTWARD.ID INNER JOIN
                         CUSTOMER ON STOCK_OUTWARD.Customer_ID = CUSTOMER.Customer_ID INNER JOIN
                         UNIT ON PRODUCT.Unit = UNIT.Unit_ID

WHERE        (DATEDIFF(day, @FromDate, STOCK_OUTWARD.RefDate) &gt;= 0) AND (DATEDIFF(day, @ToDate, STOCK_OUTWARD.RefDate) &lt;= 0)</CommandText>
