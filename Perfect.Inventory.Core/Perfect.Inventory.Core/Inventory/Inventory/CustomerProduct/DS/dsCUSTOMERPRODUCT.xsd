                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        CUSTOMER.Customer_ID, CUSTOMER.CustomerName, PRODUCT.Product_ID, PRODUCT.Product_Name, PRODUCT.Unit, UNIT.Unit_Name, 
                         SUM(STOCK_OUTWARD_DETAIL.Quantity) AS Quantity, SUM(STOCK_OUTWARD_DETAIL.UnitPrice) AS UnitPrice, SUM(STOCK_OUTWARD_DETAIL.Amount) 
                         AS Amount, SUM(STOCK_OUTWARD_DETAIL.Cost) AS Cost
FROM            STOCK_OUTWARD_DETAIL INNER JOIN
                         STOCK_OUTWARD ON STOCK_OUTWARD_DETAIL.Outward_ID = STOCK_OUTWARD.ID INNER JOIN
                         CUSTOMER ON STOCK_OUTWARD.Customer_ID = CUSTOMER.Customer_ID INNER JOIN
                         PRODUCT ON STOCK_OUTWARD_DETAIL.Product_ID = PRODUCT.Product_ID INNER JOIN
                         UNIT ON PRODUCT.Unit = UNIT.Unit_ID
WHERE        (DATEDIFF(day, @FromDate, STOCK_OUTWARD.RefDate) &gt;= 0) AND (DATEDIFF(day, @ToDate, STOCK_OUTWARD.RefDate) &lt;= 0)
GROUP BY CUSTOMER.Customer_ID, CUSTOMER.CustomerName, PRODUCT.Product_ID, PRODUCT.Product_Name, PRODUCT.Unit, UNIT.Unit_Name</CommandText>
